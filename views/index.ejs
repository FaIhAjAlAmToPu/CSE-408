    <link rel="stylesheet" href="css/style.css" />
    <div>
      <link href="css/index.css" rel="stylesheet" />

      <div class="logincontainer-container">
        <div class="logincontainer-logincontainer">
          <div class="logincontainer-logindetails">
            <div class="logincontainer-inputcollection">
              <div class="logincontainer-unilever-logo">
                <img
                  alt="image49341"
                  src="https://upload.wikimedia.org/wikipedia/en/e/e4/Unilever.svg"
                  class="logincontainer-image4"
                />
              </div>
              <form action="/" method="POST">
                <div class="logincontainer-group4">
                  <div class="logincontainer-rectangle34">
                    <span class="logincontainer-text"
                      ><span>Username*</span></span
                    >
                    <div class="logincontainer-rectangle341">
                      <input
                        class="email-cls"
                        type="text"
                        id="email"
                        name="username"
                        placeholder="Enter your username"
                      />
                    </div>
                  </div>
                  <div class="logincontainer-rectangle35">
                    <span class="logincontainer-text"
                      ><span>Password*</span></span
                    >
                    <div class="logincontainer-rectangle341">
                      <input
                        class="email-cls"
                        type="password"
                        id="pwd"
                        name="password"
                        placeholder="Enter your password"
                      />
                    </div>
                  </div>
                </div>
                <div class="logincontainer-rectangle39">
                  <div class="logincontainer-group3">
                    <input
                      class="logincontainer-rectangle391"
                      type="checkbox"
                      id="remember-me"
                      name="remember-me"
                      value="checkbox"
                    />
                    <span class="logincontainer-text08">
                      <span>Remember me</span>
                    </span>
                  </div>
                  <span class="logincontainer-text10">
                    <span>Forgot password?</span>
                  </span>
                </div>
                <div class="logincontainer-rectangle343">
                  <span class="logincontainer-text12"
                    ><input class="submit-cls" type="submit" value="Login"
                  /></span>
                </div>
              </form>
            </div>
          </div>
          <div class="logincontainer-background-image">
            <img
              alt="image39323"
              src="https://topo.cc/wp-content/uploads/2022/05/SCM_2_updated_new.jpg"
              width="300%"
            />
          </div>
        </div>
      </div>
    </div>
